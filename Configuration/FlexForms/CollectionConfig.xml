<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Options</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.choosenCollection>
                        <TCEforms>
                            <label>i18n: The Collection that is to be displayed</label>
                            <config>
                                <type>select</type>
                                <itemsProcFunc>Ubl\SparqlToucan\Controller\FrontController->PluginList</itemsProcFunc>
                                <renderType>selectSingle</renderType>
                                <items>
                                    <!-- empty by default -->
                                </items>
                            </config>
                        </TCEforms>
                    </settings.choosenCollection>
                    <settings.collection2>
                        <TCEforms>
                            <label>Collection but with search</label>
                            <config>
                                <type>user</type>
                                <userFunc>Ubl\SparqlToucan\Utility\SearchLoading->customfield</userFunc>
                            </config>
                        </TCEforms>
                    </settings.collection2>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>